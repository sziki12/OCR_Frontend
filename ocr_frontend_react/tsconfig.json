{
  "compilerOptions": {
    "module": "NodeNext",
    "removeComments": true,
    "preserveConstEnums": true,
    "moduleResolution": "nodenext",
    "sourceMap": true,
    "outDir": "src/dist",
    "allowJs": true,
  },
  "include": ["src/endpoints/*"],
  "exclude": ["**/*.spec.ts","**/*.js","src/dist/**/*"],
}
